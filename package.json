{
  "name": "yonash.dev",
  "version": "0.0.0",
  "description": "Frontend part of the Gowebly project.",
  "license": "MIT",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "fmt": "prettier --write .",
    "build": "parcel build ./internal/web/assets/scripts.js ./internal/web/assets/styles.scss --dist-dir ./internal/web/static",
    "dev": "parcel build ./internal/web/assets/scripts.js ./internal/web/assets/styles.scss --dist-dir ./internal/web/static --no-optimize",
    "watch": "parcel watch ./internal/web/assets/scripts.js ./internal/web/assets/styles.scss --dist-dir ./internal/web/static --no-optimize"
  },
  "dependencies": {
    "@tailwindcss/forms": "latest",
    "@tailwindcss/typography": "latest",
    "flowbite": "^2.5.1",
    "htmx.org": "latest",
    "tailwindcss": "latest"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "latest",
    "autoprefixer": "latest",
    "parcel": "latest",
    "postcss": "latest",
    "prettier": "latest"
  }
}
